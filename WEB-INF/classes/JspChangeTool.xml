<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>

    <package name="JspChangeTool" namespace="/" extends="default">

        <action name="Index" class="IndexAction" method="initPage">
            <result>/jsp/main.jsp</result>
        </action>
        
        <!-- Plupload Queue Widget action start -->

        <action name ="FileUpload" class ="FileUploadAction" method="upload">
            <param name="savePath">/fileUpload</param> <!-- pass the file savepath param to the action -->
            
            <result name ="success">/jsp/result.jsp</result> <!-- this result do nothing -->
            
        </action >
        <action name ="Submit" class ="FileUploadAction" method="submit">
            <param name="savePath">/fileUpload</param> <!-- pass the file savepath param to the action -->
            <result name ="success">/jsp/result.jsp</result>
        </action >

        <!-- Plupload Queue Widget action end -->
        
        
        
        <!--upload project and unzip action start 20140919-->

        <action name ="ProjectUpload" class ="ProjectUploadAction" method="upload">
            <param name="savePath">/fileUpload</param> <!-- pass the file savepath param to the action -->
            <result name ="success">/jsp/result.jsp</result> <!-- this result do nothing -->
        </action >
        <action name ="SubmitProject" class ="ProjectUploadAction" method="submit">
            <param name="savePath">/fileUpload</param> <!-- pass the file savepath param to the action -->
            <result name ="success">/jsp/result.jsp</result>
        </action >

        <!--upload project and unzip action end 20140919 -->
        
        
        <!--create template action start 20141003 -->
         <action name="preview" class="CreateTemplateAction" method="execute">
                <result name ="success">/jsp/result.jsp</result>
         </action>
        <!--create template action end 20141003 -->
        
        <!--saveFileToDb action start 20141010 -->
         <action name="saveFileToDb" class="FileUploadToDbAction" method="upload">
                <param name="savePath">/fileUpload</param> <!-- pass the file savepath param to the action -->
            <result name ="success">/jsp/result.jsp</result> <!-- this result do nothing -->
        </action >
        <action name ="SubmitFileToDb" class ="FileUploadToDbAction" method="submit">
            <param name="savePath">/fileUpload</param> <!-- pass the file savepath param to the action -->
            <result name ="success">/jsp/result.jsp</result>
        </action >
        
        <!--saveFileToDb action end 20141010 -->
        
        
        

        <!-- Add actions here -->
         
                
    </package>
    
   

</struts>
