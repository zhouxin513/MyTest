<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>

    <package name="JspChangeTool" namespace="/" extends="default">

        <action name="HelloWorld" class="HelloWorld" method="initPage">
            <result>/jsp/HelloWorld.jsp</result>
        </action>
        
        <!-- Plupload Queue Widget action start -->

        <action name ="FileUpload" class ="FileUploadAction" method="upload">
            <param name="savePath">/fileUpload</param> <!-- pass the file savepath param to the action -->
            
            <result name ="success">/jsp/result.jsp</result> <!-- this result do nothing -->
            
        </action >
        <action name ="Submit" class ="FileUploadAction" method="submit">
            <param name="savePath">/fileUpload</param> <!-- pass the file savepath param to the action -->
            <result name ="success">/jsp/result.jsp</result>
        </action >

        <!-- Plupload Queue Widget action end -->
        
        
        
        <!-- file upload and format action start 20140819-->

        <action name ="FileUploadFormat" class ="FileUploadActionFormatter" method="upload">
            <param name="savePath">/fileUpload</param> <!-- pass the file savepath param to the action -->
            <result name ="success">/jsp/result.jsp</result> <!-- this result do nothing -->
        </action >
        <action name ="SubmitFormat" class ="FileUploadActionFormatter" method="submit">
            <param name="savePath">/fileUpload</param> <!-- pass the file savepath param to the action -->
            <result name ="success">/jsp/result.jsp</result>
        </action >

        <!-- Plupload Queue Widget action end -->
        
        
        
        

        <!-- Add actions here -->
         
                
    </package>
    
   

</struts>
